<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="Leafing.Settings" type="Leafing.Core.Setting.NameValueSectionHandler, Leafing.Core"/>
  </configSections>

  <Leafing.Settings>
    <add key="RemotingUriPrefix" value="tcp://localhost:9145/"/>
    <add key="DataBase" value="@Access : @~nothing.mdb"/>
  </Leafing.Settings>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
